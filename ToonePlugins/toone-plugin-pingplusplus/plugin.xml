		<source-file src="sdk/android/lib/pingpp/res/layout/activity_pingpp_payment.xml" target-dir="app/src/main/res/layout"/>
        <source-file src="sdk/android/lib/pingpp/res/layout/fragment_pay_way_dialog.xml" target-dir="app/src/main/res/layout"/>
