        <source-file src="sdk/android/lib/pingpp/res/layout/pingpp_activity_pay.xml" target-dir="app/src/main/res/layout"/>
        <source-file src="sdk/android/lib/pingpp/res/layout/pingpp_bill_item.xml" target-dir="app/src/main/res/layout"/>
        <source-file src="sdk/android/lib/pingpp/res/layout/pingpp_pay_channel_item.xml" target-dir="app/src/main/res/layout"/>
		
		<source-file src="sdk/android/lib/pingpp/res/values/pingpp_colors.xml" target-dir="app/src/main/res/values"/>
        <source-file src="sdk/android/lib/pingpp/res/values/pingpp_attrs.xml" target-dir="app/src/main/res/values"/>
        <source-file src="sdk/android/lib/pingpp/res/values/pingpp_strings.xml" target-dir="app/src/main/res/values"/>
        <source-file src="sdk/android/lib/pingpp/res/values/pingpp_styles.xml" target-dir="app/src/main/res/values"/>
		
    </platform>
   
</plugin>
