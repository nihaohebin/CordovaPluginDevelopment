        <source-file src="sdk/android/lib/pingpp/res/drawable-xhdpi/pingpp_jingdongpay.png" target-dir="app/src/main/res/drawable-xhdpi"/>
        <source-file src="sdk/android/lib/pingpp/res/drawable-xhdpi/pingpp_nocard.png" target-dir="app/src/main/res/drawable-xhdpi"/>
        <source-file src="sdk/android/lib/pingpp/res/drawable-xhdpi/pingpp_qq.png" target-dir="app/src/main/res/drawable-xhdpi"/>
        <source-file src="sdk/android/lib/pingpp/res/drawable-xhdpi/pingpp_success.png" target-dir="app/src/main/res/drawable-xhdpi"/>
        <source-file src="sdk/android/lib/pingpp/res/drawable-xhdpi/pingpp_unionpay.png" target-dir="app/src/main/res/drawable-xhdpi"/>
        <source-file src="sdk/android/lib/pingpp/res/drawable-xhdpi/pingpp_unipay.png" target-dir="app/src/main/res/drawable-xhdpi"/>
